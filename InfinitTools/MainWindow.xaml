        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InfinitTools"
        mc:Ignorable="d"
        Title="InfiniTools" Height="600" Width="800">

    <Grid>
