<Page x:Class="InfinitTools.Pages.PortalPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:InfinitTools.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="700" d:DesignWidth="1000"
      Title="PortalPage">

    <Grid>

        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="30" Margin="395,279,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="200"/>
        <PasswordBox x:Name="passwordBox"  Height="30" HorizontalAlignment="Left" Margin="395,330,0,0" VerticalAlignment="Top" Width="200"/>
        <Label x:Name="Username" Content="Username" HorizontalAlignment="Left" Margin="292,283,0,0" VerticalAlignment="Top" Width="103"/>
        <Label x:Name="label" Content="Password" HorizontalAlignment="Left" Margin="292,334,0,0" VerticalAlignment="Top" Width="103"/>
        <Button x:Name="button" Content="Login" HorizontalAlignment="Left" Margin="519,383,0,0" VerticalAlignment="Top" Width="76" Height="30" Click="Button_Click" />

    </Grid>
</Page>
